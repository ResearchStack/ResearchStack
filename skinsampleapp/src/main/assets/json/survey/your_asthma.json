      "guid": "21987ad2-4846-48a4-ba80-27b3371186c0",
      "type": "SurveyQuestion",
      "constraints": {
        "dataType": "integer",
        "type": "IntegerConstraints",
        "step": null,
        "maxValue": 900,
        "minValue": 60
      }
    },
