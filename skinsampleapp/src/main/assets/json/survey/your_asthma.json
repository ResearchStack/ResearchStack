      "guid": "854cff00-3b2f-41c2-9b88-17b72287819a",
      "type": "SurveyQuestion",
      "constraints": {
        "rules": [
          {
            "operator": "eq",
            "skipTo": "plan",
            "type": "SurveyRule",
            "value": 1
          }
        ],
        "dataType": "boolean",
        "type": "BooleanConstraints"
      }
    },
    {
      "identifier": "flu_prompt",
      "prompt": "Did you know: Flu vaccine is recommended for most people with asthma?",
