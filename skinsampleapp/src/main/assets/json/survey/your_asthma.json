      "type": "SurveyQuestion",
      "constraints": {
        "dataType": "integer",
        "type": "MultiValueConstraints",
        "enumeration": [
          {
            "type": "SurveyQuestionOption",
            "value": 1,
            "label": "Yes"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 2,
            "label": "No"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 3,
            "label": "Not sure"
          }
        ]
      }
    },
    {
      "identifier": "peak_flow",
      "prompt": "If you use your peak flow meter to monitor your asthma, enter your best peak flow (L/min)",
