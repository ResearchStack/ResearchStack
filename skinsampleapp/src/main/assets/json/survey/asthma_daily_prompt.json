      "type": "SurveyQuestion",
      "constraints": {
        "dataType": "integer",
        "allowMultiple": true,
        "type": "MultiValueConstraints",
        "enumeration": [
          {
            "type": "SurveyQuestionOption",
            "value": 1,
            "label": "A cold"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 2,
            "label": "Exercise"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 3,
            "label": "Being more active than usual (walking, running, climbing stairs)"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 4,
            "label": "Strong smells (perfume, chemicals, sprays, paint)"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 5,
            "label": "Exhaust fumes"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 6,
            "label": "House dust"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 7,
            "label": "Dogs"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 8,
            "label": "Cats"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 9,
            "label": "Other furry/feathered animals"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 10,
            "label": "Mold"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 11,
            "label": "Pollen from trees, grass or weeds"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 12,
            "label": "Extreme heat"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 13,
            "label": "Extreme cold"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 14,
            "label": "Changes in weather"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 15,
            "label": "Around the time of my period"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 16,
            "label": "Poor air quality"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 17,
            "label": "Someone smoking near me"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 18,
            "label": "Stress"
          },
          {
            "type": "SurveyQuestionOption",
            "value": 19,
            "label": "Feeling sad, angry, excited, tense"
