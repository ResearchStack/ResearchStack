    <!-- Timed walk active task. -->
    <string name="rsb_TIMED_WALK_TITLE">Caminata cronometrada</string>
    <string name="rsb_TIMED_WALK_INTRO_DETAIL">Esta actividad mide la función de tus extremidades inferiores.</string>
    <string name="rsb_timed_walk_intro_2_text">Encuentra un lugar, de preferencia al aire libre, donde puedas caminar alrededor de %1$s en línea recta tan rápido como sea posible pero de forma segura. No te detengas sino hasta cruzar la meta.</string>
    <string name="rsb_TIMED_WALK_INTRO_2_DETAIL">Toca Siguiente para comenzar.</string>
    <string name="rsb_TIMED_WALK_FORM_TITLE">Dispositivo de ayuda</string>
    <string name="rsb_TIMED_WALK_FORM_TEXT">Usa el mismo dispositivo de ayuda para cada prueba.</string>
    <string name="rsb_TIMED_WALK_QUESTION_TEXT">¿Usas una órtesis para pie y tobillo?</string>
    <string name="rsb_TIMED_WALK_QUESTION_2_TITLE">¿Usas un dispositivo de ayuda?</string>
    <string name="rsb_TIMED_WALK_QUESTION_2_TEXT">Toca aquí para seleccionar una respuesta.</string>
