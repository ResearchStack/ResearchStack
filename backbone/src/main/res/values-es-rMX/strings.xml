    <!-- Tower of Hanoi active task -->
    <string name="rsb_TOWER_OF_HANOI_TASK_TITLE">Torre de Hanói</string>
    <string name="rsb_TOWER_OF_HANOI_TASK_INTENDED_USE">Esta actividad evalúa tu capacidad para resolver acertijos.</string>
    <string name="rsb_TOWER_OF_HANOI_TASK_INTRO_TEXT">Mueve toda la pila a la plataforma iluminada usando un número mínimo de movimientos.</string>
