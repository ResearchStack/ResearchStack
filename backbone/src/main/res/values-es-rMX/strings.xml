    <string name="rsb_TIMED_WALK_QUESTION_2_CHOICE_3">Muleta unilateral</string>
    <string name="rsb_TIMED_WALK_QUESTION_2_CHOICE_4">Bastón bilateral</string>
    <string name="rsb_TIMED_WALK_QUESTION_2_CHOICE_5">Muleta bilateral</string>
