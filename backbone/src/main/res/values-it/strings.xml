    <!-- Tower of Hanoi active task -->
    <string name="rsb_TOWER_OF_HANOI_TASK_TITLE">Torre di Hanoi</string>
    <string name="rsb_TOWER_OF_HANOI_TASK_INTENDED_USE">Questa attività valuta le tue capacità di risolvere rompicapi.</string>
